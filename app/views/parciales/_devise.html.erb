<div class="row white">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name),html:{:id=>"iniciar_sesion" ,:class =>"col s12 "}) do |f| %>
    <div class="row">
      <div class="input-field col s12 cero-padding">
        <i class="fa fa-envelope prefix"></i>
        <%= f.email_field :email, autofocus: true,:id=>"users_email" %>
        <%= f.label :email,"Correo electronico" %>
      </div>

      <div class="input-field col s12 cero-padding">
        <i class="fa fa-user-secret prefix"></i>
        <%= f.password_field :password, autocomplete: "off",:id=>"users_password" %>
        <%= f.label :password,"Contraseña" %>
      </div>

      
      <div class="actions col s12 left-align cero-padding" >
        <% if devise_mapping.rememberable? -%>
          <div class="field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
          </div>
        <% end -%> 
      </div>
    
      <div class="actions col s12 right-align  cero-padding">
         <button class="btn waves-effect waves-light valign btn-ingresar" type="submit" name="action" style="width: 100%;height:35px;">
         INICIAR SESIÓN
          <i class="fa fa-paper-plane right"></i>
        </button>
      </div>
      <div class="center-align col s12 cero-padding">
        <%= render "devise/shared/links" %>
        <div class="row">
          <%= link_to "Sign in with Facebook",user_facebook_omniauth_authorize_path(:facebook) %>
        </div>
        <div class="row">
          <%= link_to "Sign in with Facebook",user_twitter_omniauth_authorize_path(:twitter) %>
        </div>
      </div>
    </div>
  <% end %>
</div>